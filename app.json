{
  "expo": {
    "name": "صوت",
    "slug": "sawt",
    "scheme": "sawt",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "backgroundColor": "#121212",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0,
      "checkAutomatically": "NEVER",
      "enabled": false
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "buildNumber": "1.0.0",
      "supportsTablet": true,
      "bundleIdentifier": "com.haithamassoli.sawt"
    },
    "android": {
      "versionCode": 2,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.haithamassoli.sawt"
    },
    "experiments": {
      "tsconfigPaths": true,
      "typedRoutes": true
    },
    "extra": {
      "firebase": {
        "apiKey": "AIzaSyC9EhXED9zGPNbS66gGlMKH7rgEBtVu5x8",
        "authDomain": "sawt-jo.firebaseapp.com",
        "projectId": "sawt-jo",
        "storageBucket": "sawt-jo.appspot.com",
        "messagingSenderId": "538339777554",
        "appId": "1:538339777554:web:ef5efccf3ef1b36dcd0ccb"
      },
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "a6128180-4a53-4099-a116-2e827f1f9c9c"
      }
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app needs access to your photos to upload the photos with the complaint"
        }
      ],
      "expo-router"
    ]
  }
}
